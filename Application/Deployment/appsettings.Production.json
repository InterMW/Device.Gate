{
    "Application": {
        "Name": "Device.Gate",
        "Version": "0.0.0.0"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
